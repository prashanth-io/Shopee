


<div class="card text-center">
  <div class="card-header">
    
  </div>
  <div class="card-body">
    <h5 class="card-title"> <%= @friend.first_name %> <%= @friend.last_name %></h5>
    <p class="card-text"><%= @friend.email %></p>
    <p class="card-text"><%= @friend.phone %></p>
    <p class="card-text"><%= @friend.twitter %></p>
